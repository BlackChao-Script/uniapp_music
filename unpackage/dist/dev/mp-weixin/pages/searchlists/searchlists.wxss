.content .content_music .music_list {
  margin-top: 10rpx;
  padding: 20rpx;
}
.content .content_music .music_list .list_name {
  font-size: 30rpx;
}
.content .content_music .music_list .list_title {
  font-size: 25rpx;
  color: #ccc;
}
.content .content_playlists .playlist_list {
  margin-left: 20rpx;
  margin-top: 10rpx;
  display: flex;
}
.content .content_playlists .playlist_list .list_pic {
  flex: 1;
}
.content .content_playlists .playlist_list .list_pic image {
  width: 80%;
  border-radius: 20rpx;
}
.content .content_playlists .playlist_list .list_namea {
  margin-top: 50rpx;
  flex: 2;
}
.content .content_playlists .playlist_list .list_namea .name_a {
  font-size: 28rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.content .content_playlists .playlist_list .list_namea .name_b {
  font-size: 25rpx;
}
.content .content_playlists .playlist_list .list_icon {
  flex: 0.5;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .content_videos .videos_list {
  margin-left: 20rpx;
  margin-top: 10rpx;
  display: flex;
}
.content .content_videos .videos_list .list_pica {
  flex: 1;
}
.content .content_videos .videos_list .list_pica image {
  width: 100%;
  border-radius: 20rpx;
}
.content .content_videos .videos_list .list_nameb {
  flex: 1;
  margin-top: 50rpx;
  margin-left: 15rpx;
}
.content .content_videos .videos_list .list_nameb .nameb_title {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.content .content_videos .videos_list .list_nameb .nameb_name {
  font-size: 28rpx;
}

