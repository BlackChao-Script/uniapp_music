.search {
  background-color: #fbe8e7;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search navigator {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search navigator .search_content {
  width: 90%;
  height: 80%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ccc;
  border-radius: 30rpx;
  background-color: #fff;
}
.search navigator .search_content .search_icon {
  font-size: 40rpx;
}

